<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>DashboardServlet</servlet-name>
    <servlet-class>com.excilys.cdb.database.servlets.DashboardServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DashboardServlet</servlet-name>
    <url-pattern>/dashboard</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddComputerServlet</servlet-name>
    <servlet-class>com.excilys.cdb.database.servlets.AddComputerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddComputerServlet</servlet-name>
    <url-pattern>/addComputer</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditComputerServlet</servlet-name>
    <servlet-class>com.excilys.cdb.database.servlets.EditComputerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditComputerServlet</servlet-name>
    <url-pattern>/editComputer</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>DeleteCompanyServlet</servlet-name>
    <servlet-class>com.excilys.cdb.database.servlets.DeleteCompanyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteCompanyServlet</servlet-name>
    <url-pattern>/deleteCompany</url-pattern>
  </servlet-mapping>
</web-app>